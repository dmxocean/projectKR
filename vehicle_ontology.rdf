    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <!-- Base Class Definitions -->
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#Vehicle"/>
    
    <!-- Propulsion-related Classes -->
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#PropulsionVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#Vehicle"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#ConventionalFuelVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#PropulsionVehicle"/>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:oneOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Diesel"/>
                            <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Midgrade"/>
                            <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Premium"/>
                            <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Regular"/>
                        </owl:oneOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#DieselVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#ConventionalFuelVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#ConventionalFuelVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:hasValue rdf:resource="http://www.example.org/vehicle-ontology#Diesel"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#GasolineVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#ConventionalFuelVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#ConventionalFuelVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:oneOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Midgrade"/>
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Premium"/>
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Regular"/>
                                </owl:oneOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#RegularGasolineVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#GasolineVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#GasolineVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:oneOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Midgrade"/>
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Regular"/>
                                </owl:oneOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#PremiumGasolineVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#GasolineVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#GasolineVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:hasValue rdf:resource="http://www.example.org/vehicle-ontology#Premium"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#AlternativeFuelVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#PropulsionVehicle"/>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:oneOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#CNG"/>
                            <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#E85"/>
                        </owl:oneOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#ElectrifiedVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#PropulsionVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:hasValue rdf:resource="http://www.example.org/vehicle-ontology#Electricity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasElectricity"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#HybridElectricVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#ElectrifiedVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#ElectrifiedVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasElectricity"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:oneOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Midgrade"/>
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Premium"/>
                                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#Regular"/>
                                </owl:oneOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#PureElectricVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#ElectrifiedVehicle"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/vehicle-ontology#ElectrifiedVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasFuelType"/>
                        <owl:hasValue rdf:resource="http://www.example.org/vehicle-ontology#Electricity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    
    <!-- Body Style-related Classes -->
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#BodyStyleVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#Vehicle"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.example.org/vehicle-ontology#PassengerVehicle">
        <rdfs:subClassOf rdf:resource="http://www.example.org/vehicle-ontology#BodyStyleVehicle"/>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.example.org/vehicle-ontology#hasBodyStyle"/>
                <owl:someValuesFrom>
